[31m-);[m
[32m+[m[32m// This command allows you to select an option that contains the given text or[m[41m [m
[32m+[m[32m// matches a regex.[m
[32m+[m[32mif (!Cypress.Commands._commands || !Cypress.Commands._commands['selectContaining']) {[m
[32m+[m[32m  Cypress.Commands.add([m
[32m+[m[32m    'selectContaining',[m
[32m+[m[32m    { prevSubject: 'element' },[m
[32m+[m[32m    (subject, textOrRegex) => {[m
[32m+[m[32m      const re =[m
[32m+[m[32m        textOrRegex instanceof RegExp[m
[32m+[m[32m          ? textOrRegex[m
[32m+[m[32m          : new RegExp(String(textOrRegex), 'i');[m
[32m+[m
[32m+[m[32m      const $select = Cypress.$(subject);[m
[32m+[m[32m      const $match = $select[m
[32m+[m[32m        .find('option')[m
[32m+[m[32m        .filter((_, el) => re.test(Cypress.$(el).text()));[m
[32m+[m
[32m+[m[32m      if ($match.length === 0) {[m
[32m+[m[32m        throw new Error(`selectContaining: no <option> matching ${re} found`);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      const value = $match.first().val();[m
[32m+[m[32m      return cy.wrap(subject).select(value, { force: true });[m
[32m+[m[32m    }[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
